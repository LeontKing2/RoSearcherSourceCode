   "key": "NahNotGonnaLeakMyKeyMan",
   "manifest_version": 3,
   "name": "RoSearcher",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.5",
   "web_accessible_resources": [ {
      "matches": [ "*://*.roblox.com/*" ],
      "resources": [ "/js/plugin.js", "/lib/bottleneck.js" ]
   } ]
}
