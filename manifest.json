{
   "content_scripts": [ {
      "css": [ "/css/styles.css" ],
      "js": [ "/js/inject.js" ],
      "matches": [ "*://*.roblox.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Search through the game servers for a player!",
   "host_permissions": [ "*://*.roblox.com/*", "*://*.rbxcdn.com/*" ],
